{
  "version": 2,
  "routes": [
    {
      "src": "^/static/(.*)",
      "dest": "/static/$1"
    },
    {
      "src": "^/audio/(.*)",
      "dest": "/audio/$1"
    },
    {
      "src": "^/images/(.*)",
      "dest": "/images/$1"
    },
    {
      "src": "^/favicon.ico$",
      "dest": "/favicon.ico"
    },
    {
      "src": "^/favicon192.png$",
      "dest": "/favicon192.png"
    },
    {
      "src": "^/favicon512.png$",
      "dest": "/favicon512.png"
    },
    {
      "src": "^/manifest.json$",
      "dest": "/manifest.json"
    },
    {
      "src": "^/site.webmanifest.json$",
      "dest": "/site.webmanifest.json"
    },
    {
      "src": ".*",
      "dest": "/index.html"
    }
  ]
}
